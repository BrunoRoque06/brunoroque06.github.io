{
  "name": "brunoroque06",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "audit": "lhci autorun",
    "build": "npm run pre && ng build",
    "deploy": "npm run build && gh-pages --dist dist/brunoroque06 --no-history",
    "format": "prettier --ignore-path .gitignore -w .",
    "format:check": "prettier --ignore-path .gitignore -l .",
    "pre": "node tools/main.js",
    "pre:watch": "nodemon --ext . --watch tools tools/main.js",
    "start": "npm run pre && ng serve",
    "watch": "npm run pre && ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/common": "16.1.5",
    "@angular/compiler": "16.1.5",
    "@angular/core": "16.1.5",
    "@angular/platform-browser": "16.1.5",
    "rxjs": "7.8.1",
    "tslib": "2.6.1",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.5",
    "@angular/cli": "16.1.5",
    "@angular/compiler-cli": "16.1.5",
    "@lhci/cli": "0.12.0",
    "gh-pages": "5.0.0",
    "nodemon": "3.0.1",
    "prettier": "3.0.0",
    "puppeteer": "20.9.0",
    "sharp": "0.32.4",
    "typescript": "5.1.6"
  }
}
